    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run lint:eslint && npm run lint:prettier"
